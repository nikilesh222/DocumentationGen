@model DocumentationGen.Models.ErrorViewModel

@{
    ViewData["Title"] = "Error";
}

<h1 class="text-danger">Error.</h1>

<p>Display Mode : @Model.Error.DisplayMode</p>
<p>ClientId : @Model.Error.ClientId</p>
<p>Error : @Model.Error.Error</p>
<p>Description : @Model.Error.ErrorDescription</p>
<p>Redirect Uri : @Model.Error.RedirectUri</p>
<p>Response mode : @Model.Error.ResponseMode</p>
<p>Request Id : @Model.Error.RequestId</p>
<p>Ui Locales : @Model.Error.UiLocales</p>